/*Style for the first level menu bar*/
ul#menu-profileAL img.avatar.avatar-35.photo {
  border-radius: 100px;
}

ul#menu-profileAL > li{
  float:left;
  list-style-type:none;
  position:relative;
}

#menu-profileAL label{
  position:relative;
  display:block;
  padding:0 18px 0 12px;
  line-height:3em;
  transition:background 0.3s;
  cursor:pointer;
}

#menu-profileAL label:after{
  content:"";
  position:absolute;
  display:block;
  top:50%;
  right:5px;
  width:0;
  height:0;
  border-top:4px solid rgba(255,255,255,.5);
  border-bottom:0 solid rgba(255,255,255,.5);
  border-left:4px solid transparent;
  border-right:4px solid transparent;
  transition:border-bottom .1s, border-top .1s .1s;
  }

#menu-profileAL label:hover,
#menu-profileAL input:checked ~ label{background:rgba(0,0,0,.3);}

#menu-profileAL input:checked ~ label:after{
  border-top:0 solid rgba(255,255,255,.5);
  border-bottom:4px solid rgba(255,255,255,.5);
  transition:border-top .1s, border-bottom .1s .1s;
}

/*hide the inputs*/
#menu-profileAL input{display:none}

/*show the second levele menu of the selected voice*/
#menu-profileAL input:checked ~ ul.submen01{
  max-height:300px;
  transition:max-height 0.5s ease-in;
  z-index: 9999;
}

/*style for the second level menu*/
#menu-profileAL .submen01{
  max-height:0;
  padding:0;
  overflow:hidden;
  list-style-type:none;
  background:#444;
  box-shadow:0 0 1px rgba(0,0,0,.3);
  transition:max-height 0.5s ease-out;
  position:absolute;
  min-width:100%;
}

#menu-profileAL .submen01 li a{
  display:block;
  padding:12px;
  color:#ddd;
  text-decoration:none;
  box-shadow:0 -1px rgba(0,0,0,.5) inset;
  transition:background .3s;
  white-space:nowrap;
}

#menu-profileAL .submen01 li a:hover{
  background:rgba(0,0,0,.3);
}
